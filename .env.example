# ArgoCD Setup Environment Variables - Example Configuration
# Copy this file to .env and modify values as needed

# Cluster Configuration
ARGOCD_CLUSTER_NAME="argocd-cluster"
DEV_CLUSTER_NAME="dev-cluster"
ARGOCD_NAMESPACE="argocd"

# Network Configuration
ARGOCD_PORT="8080"
INTERFACE_NAME="eth0"

# Repository Configuration
MAIN_REPO_URL="https://github.com/PiotrSacharuk/argocd-training-public-repo.git"
CONFIG_REPO_URL="https://github.com/PiotrSacharuk/ArgoCD.git"

# ArgoCD Configuration
ARGOCD_USERNAME="admin"
PROJECT_NAME="dev-argocd"
DEFAULT_NAMESPACE="default"

# K3s/kubectl Configuration
K3S_COMPATIBLE_VERSION="v1.31.0"

# Docker Configuration
DOCKER_GROUP="docker"